#define ADD_FIXED_64(x, y) (x + y)
#define SUB_FIXED_64(x, y) (x - y)

#define MUL_FIXED_64(x, y) ((x * y) >> FRACTION_BITS_64)
#define DIV_FIXED_64(x, y) ((x << FRACTION_BITS_64) / y)

