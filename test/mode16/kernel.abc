#define SCREEN_WIDTH 80
#define SCREEN_HEIGHT 25

U0 putc(I8 c, U8 x, U8 y)
{
    I16 offs = (y * SCREEN_WIDTH + x) * 2;

    __asm
    {
        mov ax, 0xB800
        mov es, ax
        mov bx, word 0
        mov al, byte (c)
        mov [es:bx], al
    }

    return;
}

U0 main(U0)
{
    putc('L', 'L', 'L');
    return;
}